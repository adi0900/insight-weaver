{
    "name": "api-server",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "eslint src --ext .ts",
        "typecheck": "tsc --noEmit",
        "test": "vitest",
        "test:tableau": "tsx src/scripts/test-tableau.ts",
        "test:salesforce": "tsx src/scripts/test-salesforce.ts",
        "test:integration": "vitest run --config vitest.integration.config.ts"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "helmet": "^8.0.0",
        "ioredis": "^5.4.2",
        "jsforce": "^3.6.2",
        "jsonwebtoken": "^9.0.2",
        "marked": "^15.0.4",
        "morgan": "^1.10.0",
        "pdfkit": "^0.16.0",
        "puppeteer": "^23.10.4",
        "uuid": "^11.0.4",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.10.2",
        "@types/pdfkit": "^0.13.5",
        "@types/uuid": "^10.0.0",
        "eslint": "^8.57.0",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
    }
}